          },
          "black": {
            "$type": "color",
            "$value": "#000000"
          }
        }
      },
      "font": {
        "family": {
          "main": {
            "$type": "fontFamily",
            "$value": "Inter"
          }
        },
        "weight": {
          "regular": {
            "$type": "fontWeight",
            "$value": "400"
          },
          "bold": {
            "$type": "fontWeight",
            "$value": "700"
          }
        }
      }
    }
  },
  "alias": {
    "alias": {
      "color": {
        "primary": {
          "$type": "color",
          "$value": "{brand.color.gold.500}"
        },
        "background": {
          "$type": "color",
          "$value": "#181818"
        },
        "card-background": {
          "$type": "color",
          "$value": "#212121"
        },
        "info": {
          "$type": "color",
          "$value": "{brand.color.blue.500}"
        },
        "success": {
          "$type": "color",
          "$value": "{brand.color.green.500}"
        },
        "error": {
          "$type": "color",
          "$value": "{brand.color.red.500}"
        },
        "warning": {
          "$type": "color",
          "$value": "{brand.color.orange.500}"
        }
      },
      "typography": {
        "base-size": {
          "$type": "dimension",
          "$value": "16px"
        }
      }
    }
  },
  "mapped": {
    "mapped": {
      "surface": {
        "default": {
          "$type": "color",
          "$value": "{alias.color.background}"
        },
        "card": {
          "$type": "color",
          "$value": "{alias.color.card-background}"
        }
      },
      "text": {
        "hero": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{brand.font.family.main}",
            "fontWeight": "{brand.font.weight.bold}",
            "fontSize": "48px"
          }
        },
        "body": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{brand.font.family.main}",
            "fontSize": "{alias.typography.base-size}",
            "lineHeight": "1.5"
          }
        }
      },
      "button": {
        "primary": {
          "background": {
            "$type": "color",
            "$value": "{alias.color.primary}"
          },
          "on-color": {
            "$type": "color",
            "$value": "{brand.color.white & black.black}"
          }
        }
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": ["brand", "alias", "mapped"]
  }
}
