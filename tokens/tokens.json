{
  "brand": {
    "brand": {
      "color": {
        "gold": {
          "50": { "$type": "color", "$value": "#fbf7ec" },
          "100": { "$type": "color", "$value": "#f2e6c4" },
          "200": { "$type": "color", "$value": "#ebd9a8" },
          "300": { "$type": "color", "$value": "#e2c880" },
          "400": { "$type": "color", "$value": "#ddbd68" },
          "500": { "$type": "color", "$value": "#d4ad42" },
          "600": { "$type": "color", "$value": "#c19d3c" },
          "700": { "$type": "color", "$value": "#977b2f" },
          "800": { "$type": "color", "$value": "#755f24" },
          "900": { "$type": "color", "$value": "#59491c" }
        },
        "slate": {
          "100": {
            "$type": "color",
            "$value": "#f1f5f9"
          },
          "900": {
            "$type": "color",
            "$value": "#0f172a"
          }
        },
        "grey": {
          "100": {
            "$type": "color",
            "$value": "#f3f4f6"
          },
          "900": {
            "$type": "color",
            "$value": "#111827"
          }
        },
        "blue": {
          "500": {
            "$type": "color",
            "$value": "#3b82f6"
          }
        },
        "green": {
          "500": {
            "$type": "color",
            "$value": "#22c55e"
          }
        },
        "red": {
          "500": {
            "$type": "color",
            "$value": "#ef4444"
          }
        },
        "orange": {
          "500": {
            "$type": "color",
            "$value": "#f97316"
          }
        },
        "white & black": {
          "white": {
            "$type": "color",
            "$value": "#ffffff"
          },
          "black": {
            "$type": "color",
            "$value": "#000000"
          }
        }
      },
      "font": {
        "family": {
          "main": {
            "$type": "fontFamily",
            "$value": "Inter"
          }
        },
        "weight": {
          "regular": {
            "$type": "fontWeight",
            "$value": "400"
          },
          "bold": {
            "$type": "fontWeight",
            "$value": "700"
          }
        }
      }
    }
  },
  "alias": {
    "alias": {
      "color": {
        "primary": {
          "$type": "color",
          "$value": "{brand.color.gold.500}"
        },
        "background": {
          "$type": "color",
          "$value": "#181818"
        },
        "card-background": {
          "$type": "color",
          "$value": "#212121"
        },
        "info": {
          "$type": "color",
          "$value": "{brand.color.blue.500}"
        },
        "success": {
          "$type": "color",
          "$value": "{brand.color.green.500}"
        },
        "error": {
          "$type": "color",
          "$value": "{brand.color.red.500}"
        },
        "warning": {
          "$type": "color",
          "$value": "{brand.color.orange.500}"
        }
      },
      "typography": {
        "base-size": {
          "$type": "dimension",
          "$value": "16px"
        }
      }
    }
  },
  "mapped": {
    "mapped": {
      "surface": {
        "default": {
          "$type": "color",
          "$value": "{alias.color.background}"
        },
        "card": {
          "$type": "color",
          "$value": "{alias.color.card-background}"
        }
      },
      "text": {
        "hero": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{brand.font.family.main}",
            "fontWeight": "{brand.font.weight.bold}",
            "fontSize": "48px"
          }
        },
        "body": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{brand.font.family.main}",
            "fontSize": "{alias.typography.base-size}",
            "lineHeight": "1.5"
          }
        }
      },
      "button": {
        "primary": {
          "background": {
            "$type": "color",
            "$value": "{alias.color.primary}"
          },
          "on-color": {
            "$type": "color",
            "$value": "{brand.color.white & black.black}"
          }
        }
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": ["brand", "alias", "mapped"]
  }
}
